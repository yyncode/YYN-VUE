<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yyn.store.mapper.ShopUploadFileMapper">
    
    <resultMap type="ShopUploadFile" id="ShopUploadFileResult">
        <result property="fileId"    column="file_id"    />
        <result property="groupId"    column="group_id"    />
        <result property="channel"    column="channel"    />
        <result property="storage"    column="storage"    />
        <result property="domain"    column="domain"    />
        <result property="fileType"    column="file_type"    />
        <result property="fileName"    column="file_name"    />
        <result property="filePath"    column="file_path"    />
        <result property="fileSize"    column="file_size"    />
        <result property="fileExt"    column="file_ext"    />
        <result property="cover"    column="cover"    />
        <result property="uploaderId"    column="uploader_id"    />
        <result property="isRecycle"    column="is_recycle"    />
        <result property="isDelete"    column="is_delete"    />
        <result property="storeId"    column="store_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectShopUploadFileVo">
        select file_id, group_id, channel, storage, domain, file_type, file_name, file_path, file_size, file_ext, cover, uploader_id, is_recycle, is_delete, store_id, create_by, create_time, update_by, update_time, remark from shop_upload_file
    </sql>

    <select id="selectShopUploadFileList" parameterType="ShopUploadFile" resultMap="ShopUploadFileResult">
        <include refid="selectShopUploadFileVo"/>
        <where>  
            <if test="groupId != null "> and group_id = #{groupId}</if>
            <if test="channel != null "> and channel = #{channel}</if>
            <if test="storage != null  and storage != ''"> and storage = #{storage}</if>
            <if test="domain != null  and domain != ''"> and domain = #{domain}</if>
            <if test="fileType != null "> and file_type = #{fileType}</if>
            <if test="fileName != null  and fileName != ''"> and file_name like concat('%', #{fileName}, '%')</if>
            <if test="filePath != null  and filePath != ''"> and file_path = #{filePath}</if>
            <if test="fileSize != null "> and file_size = #{fileSize}</if>
            <if test="fileExt != null  and fileExt != ''"> and file_ext = #{fileExt}</if>
            <if test="cover != null  and cover != ''"> and cover = #{cover}</if>
            <if test="uploaderId != null "> and uploader_id = #{uploaderId}</if>
            <if test="isRecycle != null "> and is_recycle = #{isRecycle}</if>
            <if test="isDelete != null "> and is_delete = #{isDelete}</if>
            <if test="storeId != null "> and store_id = #{storeId}</if>
        </where>
    </select>
    
    <select id="selectShopUploadFileByFileId" parameterType="Long" resultMap="ShopUploadFileResult">
        <include refid="selectShopUploadFileVo"/>
        where file_id = #{fileId}
    </select>
    <select id="selectShopUploadFileInFileIds" resultMap="ShopUploadFileResult">
        <include refid="selectShopUploadFileVo"/>
        where file_id in
        <foreach item="fileId" collection="array" open="(" separator="," close=")">
            #{fileId}
        </foreach>
    </select>

    <insert id="insertShopUploadFile" parameterType="ShopUploadFile" useGeneratedKeys="true" keyProperty="fileId">
        insert into shop_upload_file
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="groupId != null">group_id,</if>
            <if test="channel != null">channel,</if>
            <if test="storage != null and storage != ''">storage,</if>
            <if test="domain != null and domain != ''">domain,</if>
            <if test="fileType != null">file_type,</if>
            <if test="fileName != null and fileName != ''">file_name,</if>
            <if test="filePath != null and filePath != ''">file_path,</if>
            <if test="fileSize != null">file_size,</if>
            <if test="fileExt != null and fileExt != ''">file_ext,</if>
            <if test="cover != null and cover != ''">cover,</if>
            <if test="uploaderId != null">uploader_id,</if>
            <if test="isRecycle != null">is_recycle,</if>
            <if test="isDelete != null">is_delete,</if>
            <if test="storeId != null">store_id,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="groupId != null">#{groupId},</if>
            <if test="channel != null">#{channel},</if>
            <if test="storage != null and storage != ''">#{storage},</if>
            <if test="domain != null and domain != ''">#{domain},</if>
            <if test="fileType != null">#{fileType},</if>
            <if test="fileName != null and fileName != ''">#{fileName},</if>
            <if test="filePath != null and filePath != ''">#{filePath},</if>
            <if test="fileSize != null">#{fileSize},</if>
            <if test="fileExt != null and fileExt != ''">#{fileExt},</if>
            <if test="cover != null and cover != ''">#{cover},</if>
            <if test="uploaderId != null">#{uploaderId},</if>
            <if test="isRecycle != null">#{isRecycle},</if>
            <if test="isDelete != null">#{isDelete},</if>
            <if test="storeId != null">#{storeId},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateShopUploadFile" parameterType="ShopUploadFile">
        update shop_upload_file
        <trim prefix="SET" suffixOverrides=",">
            <if test="groupId != null">group_id = #{groupId},</if>
            <if test="channel != null">channel = #{channel},</if>
            <if test="storage != null and storage != ''">storage = #{storage},</if>
            <if test="domain != null and domain != ''">domain = #{domain},</if>
            <if test="fileType != null">file_type = #{fileType},</if>
            <if test="fileName != null and fileName != ''">file_name = #{fileName},</if>
            <if test="filePath != null and filePath != ''">file_path = #{filePath},</if>
            <if test="fileSize != null">file_size = #{fileSize},</if>
            <if test="fileExt != null and fileExt != ''">file_ext = #{fileExt},</if>
            <if test="cover != null and cover != ''">cover = #{cover},</if>
            <if test="uploaderId != null">uploader_id = #{uploaderId},</if>
            <if test="isRecycle != null">is_recycle = #{isRecycle},</if>
            <if test="isDelete != null">is_delete = #{isDelete},</if>
            <if test="storeId != null">store_id = #{storeId},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where file_id = #{fileId}
    </update>

    <delete id="deleteShopUploadFileByFileId" parameterType="Long">
        delete from shop_upload_file where file_id = #{fileId}
    </delete>

    <delete id="deleteShopUploadFileByFileIds" parameterType="String">
        delete from shop_upload_file where file_id in 
        <foreach item="fileId" collection="array" open="(" separator="," close=")">
            #{fileId}
        </foreach>
    </delete>
</mapper>